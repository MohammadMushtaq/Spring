import java.util.Scanner;

public class Strin {

	public static void main(String[] args) {
		int num;int temp=0;
		Scanner sc = new Scanner(System.in);
		System.out.println("enter the array");
		num= sc.nextInt();
		
		int n2[]= new int[num];
		
		for (int i = 0; i < num; i++) {
			n2[i]=sc.nextInt();
		} 
		for (int i = 0; i < num; i++) {
			for (int j = i+1; j < num; j++) {
				
				int[] a;
				if(a[i]>a[j]) {
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
		}
		for (int i = 0; i < n2.length; i++) {
			System.out.println(n2[i]);
		}
		}	
	}	


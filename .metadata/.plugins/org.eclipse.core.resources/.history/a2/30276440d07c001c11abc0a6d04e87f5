		int num=123;int num1=num;
		int t1=0;
		int len=0;
		while(num1!=0) {
			t1=t1%num1;
			len=len+1;
		}
		int t2=num;
		int arm=0;
		while(t2!=num) {
			int mul=1;
			for (int i = 0; i <=len; i++) {
				int rem=t2%num;
				mul= mul*rem;
				
			}
			arm= arm+mul;
		}if(num==arm) {
			System.out.println(num+" "+"num is amstrong");
		}else{
			System.out.println(num+" "+"num is not amstrong");
		}
	}

}

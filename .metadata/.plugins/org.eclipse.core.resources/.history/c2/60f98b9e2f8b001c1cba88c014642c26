package com.xworkz.bean.spring;

public class Laptop {
	
	private Camera camera;
	private String brand;
	
	public Laptop() {
		System.out.println("invoked laptop no args");
	}
		
	public void setBrand(String brand) {
		this.brand = brand;
	}
	public void setCamera(Camera camera) {
		this.camera = camera;
		if(camera!=null) {
			camera.captureImage();
			System.out.println("invoked camera of "+brand);
		}else {
			System.err.println("camera is not present");
		}
		
	}	

}

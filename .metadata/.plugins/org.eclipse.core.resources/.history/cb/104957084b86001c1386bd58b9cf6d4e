import java.util.Scanner;

public class Strin {

	public static void main(String[] args) {
	int num;int temp=0;
		Scanner sc = new Scanner(System.in);
		 System.out.println("enter te number want in array");
		 num= sc.nextInt();
		 int a[]= new int[num];
		System.out.println("enter all elements");
		for (int i = 0; i < num; i++) {
			a[i]=sc.nextInt();
		}
		for (int i = 0; i < num; i++) {
			for (int j = i+1; j < num; j++) {
				if(a[i]>a[j]) {
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
		}for (int i = 0; i < a.length; i++) {
			System.out.println(a[i]);
		}
		}	
	}	


public class Strin {

	public static void main(String[] args) {
	
		
		int num=153;
		int temp= num;
		int len=0;
		while(temp!=0) {
			temp=temp/10;
			len=len+1;
			}
		int t2=num;
		int rem=0;
		int rev=0;
		while(t2!=0) {
			rem= t2%10;
			rev=rev*len+rem;
			t2=t2/10;
		}
		System.out.println(rev);
		}
		
			
	}	


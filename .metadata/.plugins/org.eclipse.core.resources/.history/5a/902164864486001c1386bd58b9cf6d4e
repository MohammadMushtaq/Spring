public class Strin {

	public static void main(String[] args) {
		
		String name="madam",temp = "";
		String name1=name;
		int len= name1.length();
		for (int i = len-1; i>=0; i--) {
			temp = temp+name1.charAt(i);
		}
		if(temp.equals(name1)) {
			System.out.println("string is palendron");
		}else {
			System.err.println("string is not palendron");}	
		}	
	}	


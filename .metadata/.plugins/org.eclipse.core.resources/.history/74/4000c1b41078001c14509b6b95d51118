		int arr[]= {2,8,2,4,4,3,9,10};
		int temp=0;
		int len= arr.length;
		for (int i = 0; i < len; i++) {
			for (int j = i+1; j < len; j++) {
				if(arr[i]==arr[j]&& arr[i]!=0) {
					temp=arr[i];
					arr[i]=arr[j];
					arr[j]=temp;
				}
			}
		}
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+" ");
		}

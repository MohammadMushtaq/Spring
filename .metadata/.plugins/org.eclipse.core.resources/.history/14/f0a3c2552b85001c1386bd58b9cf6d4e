import java.util.Scanner;

public class Strin {

	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);
		System.err.println("enter the anything ");
		int a=sc.nextInt();
	
		for (int i = a; i <5; i++) {
			for (int j = 1; j<=i; j++) {
				
				System.out.print("*");
			}
			System.out.println();
		}
	}	
}

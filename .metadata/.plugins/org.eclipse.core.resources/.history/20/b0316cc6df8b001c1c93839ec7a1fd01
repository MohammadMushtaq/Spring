package com.xworkz.bean;

import java.util.ArrayList;
import java.util.List;

public class Person {

	private String name;
	private List<String> bookNames = new ArrayList<String>();
	
	
	public Person(String name, List<String> bookNames) {
		super();
		this.name = name;
		this.bookNames = bookNames;
	}
	
	
}

package com.xworkz.travel.travels;

public class TravelsImpl implements Travels{

	private Travels travels;
	private String name;
	
	
	public void setTravels(Travels travels) {
		this.travels = travels;
		System.out.println("travels method ");
	}


	public void setName(String name) {
		this.name = name;
		System.out.println("invoked travels name ");

	}


	@Override
	public void booking() {
		travels.booking();
		System.out.println("invoked booking");
		
	}
	

}
